   "source": [
    "# simple image manipulations using PILLOW\n",
    "from PIL import Image\n",
    "\n",
    "my_file = \"data/page_01.jpg\"\n",
    "im = Image.open(my_file) # image loaded into memory\n",
    "print(im) #image meta data\n",
    "print(im.size)\n",
    "im.show()\n",
    "im.rotate(90).show()\n",
    "# save  \n",
    "im.save(\"temp/page_01.jpg\")"
   ]
